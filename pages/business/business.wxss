
.container {
    width: 100%;
    overflow-y: scroll;
    overflow-x: hidden;
    /* background-color: #F6F7FB; */
}

.test-view {
    text-align: center;
    /* background-color: #F6F7FB; */
    position: relative;
    font-size: 26rpx;
}
.business_model {
    width: 450rpx;
    border-radius: 0 0 230rpx 230rpx;
}
.item-test {
    padding: 10rpx 20rpx;
    border: 1rpx solid #ccc;
    border-radius: 30rpx;
    background-color: #FFF;
}
.item-border {
    height: 2rpx;
    background-color: #c28324;
}
.test-0 {
    position: absolute;
    top: 70rpx;
    left: 70rpx;
    vertical-align: top;
    /*display: flex;*/
}
.border-0 {
    width: 30rpx;
    height: 2rpx;
    background-color: #c28324;
    position: relative;
    left: 120rpx;
    top: -20rpx;
}
.border-0:after {
    content: '';
    /*border-top: 1rpx solid #c28324;*/
    width: 130rpx;
    height: 2rpx;
    background-color: #c28324;
    position: absolute;
    transform-origin: top left;
    transform: rotate(60deg);
    /*left: 120rpx;*/
    vertical-align: middle;
    margin-left: 15rpx;
}
.test-1 {
    position: absolute;
    top: 70rpx;
    right: 70rpx;
}
.test-2 {
    position: absolute;
    top: 220rpx;
    left: 70rpx;
}
.border-2 {
    /*border-top: 1rpx solid #c28324;*/
    width: 80rpx;
    position: absolute;
    top: 30rpx;
    left: 180rpx;
}
.test-3 {
    position: absolute;
    top: 220rpx;
    right: 70rpx;
}
.border-3 {
    /*border-top: 1rpx solid #c28324;*/
    width: 120rpx;
    position: absolute;
    right: 145rpx;
    top: 20rpx;
}
.test-4 {
    position: absolute;
    top: 330rpx;
    left: 70rpx;
}
.test-5 {
    position: absolute;
    top: 330rpx;
    right: 70rpx;
}
.border-5 {
    width: 30rpx;
    background-color: #c28324;
    position: relative;
    top: 20rpx;
    left: -30rpx;
    /*left: 120rpx;
    top: -20rpx;*/
}
.border-5:before {
    /*border-bottom: 1rpx solid #c28324;*/
    content: '';
    width: 180rpx;
    height: 2rpx;
    background-color: #c28324;
    position: absolute;
    right: 30rpx;
/*    right: 135rpx;
    top: -35rpx;*/
    transform-origin: bottom right;
    transform: rotate(40deg);
}

.business-top {
    /* background-color: #F6F7FB; */
    text-align: center;
    padding-top: 20rpx;
}
.business-img {
    width: 680rpx;
    height: 435rpx;
    text-align: center;
    border-radius: 20rpx 20rpx 0 0;
}
.prompt-icon {
    width: 50rpx;
    height: 50rpx;
    margin-right: 30rpx;
}
.business-link {
    width: 680rpx;
    font-size: 26rpx;
    background-color: #FFF;
    margin: auto;
    padding: 20rpx 0;
    border-radius: 0 0 20rpx 20rpx;
    margin-top: -10rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.business-scroll {
    display: flex;
    white-space: nowrap;
    color: #333333;
    font-size: 28rpx;
    background-color: #F6F7FB;
}
.item-dossier {
    display: inline-block;
    width: 500rpx;
    margin: 30rpx;
    background-color: #FFF;
}

.dossier-text {
    white-space: normal;
}

.sel {
    color: red;
}


