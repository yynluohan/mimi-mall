.business-nav {
    display: flex;
    flex-wrap: wrap;
    /* background-color: #F6F7FB; */
    color: #505050;
    font-size: 30rpx;
    padding-top: 30rpx;
    margin: 20rpx;
}
.item-nav {
    display: flex;
    flex-direction: column;
    width: 200rpx;
    text-align: center;
    position: relative;
    padding-bottom: 30rpx;
    background: #F6F7FB;
    margin: 20rpx 10rpx 20rpx 20rpx;
}
.border-right {
    position: absolute;
    right: 0;
    top: 10%;
    height: 80%;
    width: 2rpx;
    background-color: #e5e5e5;
}
.border-bottom {
    position: absolute;
    left: 10%;
    bottom: 0;
    height: 2rpx;
    width: 80%;
    background-color: #e5e5e5;
}
