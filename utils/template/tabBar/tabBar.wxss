/*     tab-Bar-view 导航栏样式开始       */

.tabBarView {
    display: flex;
    flex-wrap: nowrap;
    text-align: center;
    /*font-size: 0.6rem;*/
    height: 10%;
    background-color: #fff;
    border-top: 1rpx solid #ccc;
    position: fixed;
    bottom: 0;
    width: 100%;
    /*justify-content: space-around;*/
    height: 62px;
    font-family: microsoft yahei;
}

.tabBarItem {
    margin: auto 20rpx;
}

.tabBarImg {
    margin: auto;
    margin-top: 0.3rem;
    height: 32px;
    width: 32px;
}

.tabBarText {
    margin: auto;
    margin-bottom: 0.3rem;
}

/*     tab-Bar-view 导航栏样式结束      */


/*------------ shrink-tab-bar 导航栏样式开始  ------------*/

.shrinkView {
    display: flex;
    position: fixed;
    left: 30rpx;
    bottom: 30rpx;
    width: 100%;
    align-items: center;
}


.mainImg {
    border-radius: 50%;
}

.tab-shrink {
    position: fixed;
    bottom: 0;
    left: 120rpx;
    width: 100%;
    z-index: 1000;
    transform: translateX(-200%);
    transition: 400ms;

}

.tab-shrink .tab-shrink-body {
    display: flex;
    position: fixed;
    bottom: 46rpx;
    left: 0;
    border: 2rpx solid #FFFFFF;
    border-radius: 14rpx;
    padding: 5rpx 0;
    /*height: 80rpx;*/
    background: #f2f2f2;
    /*padding-top: 60rpx;*/
    /*transform: translateX(0);*/
    transition: transform 400ms;
}

.tab-shrink.active {
    transform: translateX(0);
    background-color: red;
}

.tab-shrink.active .tab-shrink-body {
    transform: translateX(0);
    transition: transform 400ms;
}

.tab-shrink-text {}

