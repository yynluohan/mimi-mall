/*------------ select下拉列表样式开始 --------------*/

.select {
    flex: 1;
    z-index: 1000;
}

.select-view {
    display: flex;
    align-items: center;
    height: 100%;
}

.select-option {
    margin-right: 10rpx;
    flex: 1;
    text-align: right;
}

.select-arrow {
    width: 0;
    height: 0;
    border-width: 15rpx 10rpx 0;
    border-style: solid;
    border-color: #333 transparent transparent;
    margin: 40rpx auto;
    position: relative;
}

.options-view {
    background-color: #FFF;
    z-index: 1000;
    text-align: right;
    width: 100%;
    border-left: 2rpx solid #ccc;
    border-right: 2rpx solid #ccc;
    border-bottom: 2rpx solid #ccc;
}

.option {
    line-height: 3;
    padding-right: 30rpx;
    border-bottom: 2rpx solid #ccc;

}

/*------------ select下拉列表样式结束 --------------*/
