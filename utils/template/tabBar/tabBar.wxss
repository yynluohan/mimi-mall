/*-----------scroll-x-packge 导航样式开始-------------*/

.scroll-x-packge {
    display: inline-block;
    /*width: 100%;*/
    flex-direction: row;
    white-space: nowrap;
    color: #333333;
    font-size: 28rpx;
    overflow-x: scroll;
    padding-left: 10rpx;
}

.item-packge {
    padding: 10rpx 0;
    margin: 0 20rpx;
    display: inline-flex;
    flex-direction: column;
    text-align: center;
    margin: 10rpx 16rpx;
}

.item-packge-img {
    width: 210rpx;
    height: 210rpx;
}

.packge-name {
    max-width: 180rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    -webkit-box-orient: vertical;
    padding: 10rpx 0;
}
.packge-price {
    font-weight: both;
}

.packge-progress-line {
  width: 100%;
  position:relative;
}

.packge-bg-progress-line {
  height: 4px;
  border-radius: 10px;
  background-color: #E7E7EA;
  width:100%;
}

.packge-font-progress-line{
  height: 4px;
  border-radius: 10px;
  background-color: #C28324;
  position: relative;
  top: -4px;
}

.packge-matchRage {
  color: rgba(0, 0, 0, 0.65);
  font-size: 22rpx;
}

.myPackge-tip {
    font-size: 30rpx;
    text-align: center;
    color: #888888;
    letter-spacing: 2rpx;
}
.myPackge-tip .tip-text {
    margin-top: 10rpx;
}

.index-appoint-btn {
    width: 202rpx;
    padding: 20rpx;
    border: 1rpx solid #333;
    border-radius: 20rpx;
    margin: 30rpx auto;

}


/*-----------scroll-x-nav导航样式结束-------------*/

